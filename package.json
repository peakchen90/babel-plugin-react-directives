{
  "name": "babel-plugin-react-directives",
  "version": "1.1.3",
  "description": "A babel plugin that provides some directives for react(any JSX), similar to directive of vue.",
  "main": "src/index.js",
  "files": [
    "src",
    "runtime",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "jest",
    "test:clear-cache": "jest --clearCache",
    "test:install": "node test/install.js && node playground/install.js",
    "pg:dev": "node playground/build.js --dev",
    "pg:build": "node playground/build.js",
    "prepublishOnly": "npm run test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js}": [
      "eslint --fix",
      "git add"
    ]
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "author": "peakchen90 <1558267774@qq.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/peakchen90/babel-plugin-react-directives"
  },
  "bugs": {
    "url": "https://github.com/peakchen90/babel-plugin-react-directives/issues"
  },
  "keywords": [
    "babel-plugin",
    "react-directive",
    "jsx-directive",
    "jsx-plus",
    "directive",
    "react",
    "vue",
    "x-if",
    "x-for",
    "v-if",
    "v-for"
  ],
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "@babel/code-frame": "^7.10.4",
    "@babel/generator": "^7.11.6",
    "@babel/types": "^7.11.5",
    "ajv": "^6.12.4",
    "ajv-errors": "^1.0.1",
    "chalk": "^4.1.0",
    "semver": "^6.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@types/jest": "^26.0.13",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "common-tags": "^1.8.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "eslint": "^7.8.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-directives": "^1.0.1",
    "husky": "^4.2.5",
    "invariant": "^2.2.4",
    "jest": "^26.4.2",
    "lint-staged": "^10.3.0",
    "lodash.mergewith": "^4.6.2",
    "path-exists": "^4.0.0",
    "prettier": "^1.18.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "strip-indent": "^3.0.0"
  }
}
